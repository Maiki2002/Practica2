.data
main:
    push {r7,lr}
    sub sp, sp, #36
    add r7, sp, #0

    @ldr r0, =message
    @mov r1, #20
    @bl leer
    @str r0,[r7]
    @Guarda datos del arreglo "Realizarlo 5 veces"

    mov r2, #0
    ldr r3, =arreglo
LOOP1:
    cmp r2, #5
    blt FOR1
    b EXIT1

FOR1:
    ldr r4, [r3], #3
    
    add r2, r2, #1
    b LOOP1

EXIT1:

    mov r3, r0
    mov r0, r3
    adds r7, r7, #36
    mov sp, r7
    pop {r7}
    bx lr

arreglo:
    .skip 20
